type Player {
  api_id: Int!
  name: String!
  lastName: String!
  firstName: String!
}

type Team {
  api_id: Int!
  name: String!
}

type UserPrivateData {
  sub: String!
  provider: String!
  token: String!
}

type User {
  battletag: String! @unique
  userPrivateData: UserPrivateData! @unique
}
